{
  "name": "autofold-comments",
  "displayName": "Autofold Comments",
  "version": "0.1.0",
  "private": false,
  "description": "Autofold Comments plugin for vscode",
  "keywords": [
    "autofold",
    "comment",
    "vscode"
  ],
  "homepage": "https://github.com/ilyub/vscode-autofold-comments",
  "bugs": {
    "url": "https://github.com/ilyub/vscode-autofold-comments/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/vscode-autofold-comments.git"
  },
  "license": "ISC",
  "author": "Ilya Lyubinskiy <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "./out/extension.js",
  "scripts": {
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "compile": "tsc",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,prod --reject @types/node",
    "update-package-check-peer": "ncu --dep peer --minimal --reject @types/node",
    "update-package-check-skylib": "ncu --dep peer --filter @skylib/*",
    "update-package-force": "ncu --dep dev,prod --reject @types/node --upgrade",
    "update-package-force-peer": "ncu --dep peer --minimal --reject @types/node --upgrade",
    "update-package-force-skylib": "ncu --dep peer --filter @skylib/* --upgrade",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch"
  },
  "contributes": {
    "commands": [
      {
        "command": "autofold-comments.paste",
        "title": "Paste + fold comments"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@skylib/eslint-plugin": "^0.1.1",
    "@skylib/config": "^0.1.4",
    "@skylib/typescript-types": "^0.1.1",
    "@types/eslint": "^7.29.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.14.0",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vscode/test-electron": "^1.6.2",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^38.0.1",
    "eslint-plugin-vue": "^8.0.3",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "npm-check-updates": "^12.0.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "vue-eslint-parser": "^8.0.1"
  },
  "engines": {
    "vscode": "^1.62.0"
  }
}
